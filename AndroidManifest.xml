<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.europa.eui"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="11" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light" >
        <activity
            android:name="com.europa.eui.activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.europa.eui.activity.ClearEditActivity"></activity>
        <activity android:name="com.europa.eui.activity.CheckActivity"
            android:theme="@android:style/Theme.Dialog"></activity>
        <activity android:name="com.europa.eui.activity.AutoActivity"
            android:launchMode="singleInstance"></activity>
    </application>

</manifest>